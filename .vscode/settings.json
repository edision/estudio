// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/dist/*": true,
    "**/public/*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "yarn.lock":true,
    ".yarnclean":true,
    "**/dist":true
  }
}
